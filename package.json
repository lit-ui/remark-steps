{
  "name": "remark-steps",
  "version": "1.1.1",
  "description": "Remark plugin to rebuild steps style into DOM",
  "style": "index.css",
  "sass": "index.scss",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "require": "./dist/index.js",
      "default": "./index.css"
    },
    "./index.css": "./index.css",
    "./index.scss": "./index.scss"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.css",
    "index.scss"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c"
  },
  "keywords": [
    "remark-plugin",
    "remark",
    "steps",
    "remark-steps",
    "remark-steps-plugin",
    "litui"
  ],
  "author": "lituidev@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lit-ui/remark-steps.git"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.9.6",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "unist-util-visit": "^5.0.0"
  }
}
